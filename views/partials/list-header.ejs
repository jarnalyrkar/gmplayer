<header>
  <h2>Add <%= type %></h2>
  <form class="add-form">
    <fieldset class="create-inputs">
      <input type="text" id="new-<%= type %>" placeholder="<%= placeholder %>" spellcheck="false">
      <button class="button button--action" data-action="add">
        <%- include('../icons/plus.svg') %>
      </button>
    </fieldset>
  </form>
</header>